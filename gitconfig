[push]
	default = simple
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[core]
	pager = less -FRX
	autocrlf = input
	excludesfile = ~/.global_gitignore
[diff "pdf"]
	textconv = hexdump -v -C
[color]
	diff = auto
	status = auto
[alias]
	adog = log --all --decorate --oneline --graph
